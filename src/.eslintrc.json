{
  "root" : true,
  "env" :{
    "node" : true,
    "es6" : true,
    "browser" : true
  },
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "next",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "react/no-unescaped-entities": 1,
    "no-unused-vars": 1,
    "no-undef": 1, 
    "react-hooks/rules-of-hooks": "warn"
  }
}
